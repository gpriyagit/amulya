pipeline {
  agent { label 'linux' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('scan') {
      steps {
        WithSonarQubeEnv(installationName: 'sonarqube') {
          sh './mvnw clean org.sonarsource.scanner.maven:Apache Maven:3.9.9:sonar'
        }
      }
    }
  }
}
